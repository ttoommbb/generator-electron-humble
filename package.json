{
  "name": "electron-humble",
  "version": "0.0.1",
  "scripts": {
    "start": "./app/node_modules/electron/dist/electron ./app/index.js prod",
    "start:dev": "./app/node_modules/electron/dist/electron ./app/index.js dev",
    "start:prod": "./app/node_modules/electron/dist/electron ./app/index.js prod",
    "build:dev": "webpack-dev-server --progress --colors --env.type=dev --mode=development",
    "build:prod": "webpack --config ./webpack.config.js --progress --colors --profile --bail --env.type=prod --mode=production",
    "tslint": "tslint -c ./tslint.json 'dev/**/*.ts'; exit 0;"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.3",
    "core-js": "^2.5.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "6.2.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/node": "^10.3.4",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "codelyzer": "^4.0.2",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "private": true
}
